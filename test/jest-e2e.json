{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@config/(.*)": "<rootDir>/../src/config/$1",
    "@db/(.*)": "<rootDir>/../src/db/$1",
    "@modules/(.*)": "<rootDir>/../src/modules/$1",
    "@swagger/(.*)": "<rootDir>/../src/swagger/$1"
  }
}
